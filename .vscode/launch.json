{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome",
      "type": "chrome",
      "request": "launch",
      "webRoot": "${workspaceRoot}",
      "sourceMaps": true,
      "url": "http://localhost:8080"
    },
    {
      "name": "Attach Chrome",
      "type": "chrome",
      "request": "attach",
      "webRoot": "${workspaceRoot}",
      "sourceMaps": true,
      "port": 9222
    },
    {
      "name": "Launch iOS Safari",
      "type": "ios",
      "request": "launch",
      "webRoot": "${workspaceRoot}",
      "sourceMaps": true,
      "port": 9222,
      "deviceName": "*",
      "url": "http://192.168.1.101:8080/*"
    },
    {
      "name": "Attach iOS Safari",
      "type": "ios",
      "request": "attach",
      "webRoot": "${workspaceRoot}",
      "sourceMaps": true,
      "port": 9222,
      "deviceName": "*",
      "url": "http://192.168.1.101:8080/*"
    }
  ]
}